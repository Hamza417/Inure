<?xml version="1.0" encoding="utf-8"?>
<app.simple.inure.decorations.corners.DynamicCornerMaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:elevation="@dimen/app_views_elevation"
    app:cardElevation="5dp"
    app:contentPadding="@dimen/popup_padding">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/information"
            android:textSize="@dimen/info_text_medium"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/icon"
                android:layout_width="@dimen/button_size"
                android:layout_height="@dimen/button_size"
                android:focusable="false"
                android:importantForAccessibility="no"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <app.simple.inure.decorations.typeface.TypeFaceTextView
                android:id="@+id/name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="10dp"
                android:layout_marginTop="16dp"
                android:ellipsize="end"
                android:maxLines="1"
                android:textSize="@dimen/app_name_text_size"
                app:appFontStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/package_name"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon"
                app:layout_constraintTop_toTopOf="@+id/icon"
                app:layout_constraintVertical_bias="0.0"
                app:textColorStyle="primary" />

            <app.simple.inure.decorations.typeface.TypeFaceTextView
                android:id="@+id/package_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="10dp"
                android:layout_marginBottom="16dp"
                android:ellipsize="end"
                android:fontFamily="@font/lato_regular"
                android:maxLines="1"
                android:textSize="@dimen/app_package_text_size"
                app:appFontStyle="regular"
                app:layout_constraintBottom_toBottomOf="@+id/icon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon"
                app:layout_constraintTop_toBottomOf="@id/name"
                app:textColorStyle="secondary" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/first_submission_date"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/first_submission_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/last_submission_date"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/last_submission_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/meaningful_name"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/meaningful_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sha256"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/sha256"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sha1"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/sha1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/md5"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/md5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/popup_padding" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/times_submitted"
            android:textSize="@dimen/info_text_small"
            app:appFontStyle="bold"
            app:textColorStyle="primary" />

        <app.simple.inure.decorations.typeface.TypeFaceTextView
            android:id="@+id/times_submitted"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/desc_text_size"
            app:appFontStyle="regular"
            app:textColorStyle="tertiary" />

    </LinearLayout>

</app.simple.inure.decorations.corners.DynamicCornerMaterialCardView>